{
  "name": "ow-mod-manager",
  "productName": "OuterWildsModManager",
  "version": "0.1.2",
  "description": "Outer Wilds Mod Manager",
  "main": ".webpack/main",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "eslint --ext .ts,.tsx,.js,.jsx .",
    "lint:fix": "eslint --fix --ext .ts,.tsx,.js,.jsx ."
  },
  "keywords": [],
  "author": {
    "name": "Ricardo Lopes",
    "email": "raicuparta@gmail.com"
  },
  "license": "MIT",
  "config": {
    "forge": {
      "packagerConfig": {
        "icon": "icon.ico",
        "win32metadata": {
          "requested-execution-level": "highestAvailable"
        },
        "extraResource": "install-update.bat"
      },
      "makers": [
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "win32"
          ]
        }
      ],
      "plugins": [
        [
          "@electron-forge/plugin-webpack",
          {
            "mainConfig": "./webpack.main.config.js",
            "renderer": {
              "config": "./webpack.renderer.config.js",
              "entryPoints": [
                {
                  "html": "./src/index.html",
                  "js": "./src/index.tsx",
                  "name": "main_window"
                }
              ]
            }
          }
        ]
      ]
    }
  },
  "devDependencies": {
    "@babel/core": "^7.10.5",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-proposal-decorators": "^7.10.5",
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-react": "^7.10.4",
    "@babel/preset-typescript": "^7.10.4",
    "@electron-forge/cli": "6.0.0-beta.52",
    "@electron-forge/maker-deb": "6.0.0-beta.52",
    "@electron-forge/maker-rpm": "6.0.0-beta.52",
    "@electron-forge/maker-zip": "6.0.0-beta.52",
    "@electron-forge/plugin-webpack": "6.0.0-beta.52",
    "@marshallofsound/webpack-asset-relocator-loader": "^0.5.0",
    "@types/fs-extra": "^9.0.1",
    "@types/lodash": "^4.14.157",
    "@types/react": "^16.9.43",
    "@types/react-dom": "^16.9.8",
    "@types/request": "^2.48.5",
    "@types/unzipper": "^0.10.3",
    "@typescript-eslint/eslint-plugin": "^3.6.1",
    "@typescript-eslint/parser": "^3.6.1",
    "babel-loader": "^8.1.0",
    "css-loader": "^3.6.0",
    "electron": "9.1.0",
    "eslint": "^7.4.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.3",
    "eslint-plugin-react-hooks": "^4.0.8",
    "fork-ts-checker-webpack-plugin": "^5.0.7",
    "node-loader": "^1.0.0",
    "prettier": "^2.0.5",
    "style-loader": "^1.2.1",
    "ts-loader": "^8.0.1",
    "typescript": "^3.9.6",
    "react-hot-loader": "^4.12.21",
    "eslint-import-resolver-typescript": "^2.0.0",
    "@hot-loader/react-dom": "^16.13.0"
  },
  "dependencies": {
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "fs-extra": "^9.0.1",
    "glob": "^7.1.6",
    "glob-promise": "^3.4.0",
    "lodash": "^4.17.19",
    "node-fetch": "^3.0.0-beta.5",
    "react": "0.0.0-experimental-33c3af284",
    "react-dom": "0.0.0-experimental-33c3af284",
    "regenerator-runtime": "^0.13.5",
    "request": "^2.88.2",
    "unzipper": "^0.10.11"
  }
}
